<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/setting_nav"
    app:startDestination="@id/settingFragment">

    <fragment
        android:id="@+id/settingFragment"
        android:name="org.projectPA.petdiary.view.fragment.setting.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_changePasswordSettingFragment"
            app:destination="@id/changePasswordSettingFragment" />
        <action
            android:id="@+id/action_settingFragment_to_editProfileSettingFragment"
            app:destination="@id/editProfileSettingFragment" />
    </fragment>
    <fragment
        android:id="@+id/changePasswordSettingFragment"
        android:name="org.projectPA.petdiary.view.fragment.setting.ChangePasswordSettingFragment"
        android:label="fragment_change_password_setting"
        tools:layout="@layout/fragment_change_password_setting" />
    <fragment
        android:id="@+id/editProfileSettingFragment"
        android:name="org.projectPA.petdiary.view.fragment.setting.EditProfileSettingFragment"
        android:label="fragment_edit_profile_setting"
        tools:layout="@layout/fragment_edit_profile_setting" />
</navigation>